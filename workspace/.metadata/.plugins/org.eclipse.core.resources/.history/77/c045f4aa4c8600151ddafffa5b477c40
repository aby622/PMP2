package test1;

public class Test {
  
  public static void main(String [] args){
	
	double zahl = 9724.0;
	System.out.println(zahl);
	zahl = zahl / 1000;
	zahl = Math.round(10.0 * zahl)/ 10.0;
	System.out.println(zahl);
	
	double zahl2 = 9724.0;
	zahl2 = (int) zahl2;
	System.out.println((int)zahl2);
  }

}
