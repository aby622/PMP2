package test1;

public class Main {
  
  public static void main(String [] args) throws InterruptedException{
	Ergebnis test1 = new Ergebnis ("test1");
	Ergebnis test2 = new Ergebnis ("test2");
	Ergebnis test3 = new Ergebnis ("test3");
	
	test1.start();
	test2.start();
	test3.start();
	
	test1.join();
	test2.join();
	test3.join();
	
	System.out.println("Test 1" + test1.getZeit());
  }

}
