package aufgabenblatt1;

/**
 * Klasse Pruefklasse
 * 
 * @author Leon & Jannes
 */
public class Pruefklasse {
	
	public static Object[] elemente;
	
	public static boolean pruefe(){
		if(elemente[0] instanceof Integer){
			return true;
		}else
			return false;
	}

}
